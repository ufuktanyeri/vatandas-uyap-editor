{
  "name": "uyap-evrak-indirici",
  "version": "2.1.0",
  "private": true,
  "type": "module",
  "description": "UYAP Vatandaş Portalı üzerinde seçilen dosyalardaki evrakları güvenli şekilde toplu indirmenizi sağlayan Chrome uzantısı.",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,css,md}\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ufuktanyeri/vatandas-uyap-editor.git"
  },

    "uyap",
    "manifest-v3",
    "document-downloader"
  ],
  "author": "Ufuk Tanyeri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ufuktanyeri/vatandas-uyap-editor/issues"
  },
  "homepage": "https://github.com/ufuktanyeri/vatandas-uyap-editor#readme",
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^28.1.0",
    "prettier": "^3.8.1",
    "vitest": "^4.0.18"
  }
}
